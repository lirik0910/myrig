{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": [
			"src"
		],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(node_modules/|docs)"
	},
	"sourceType": "module",
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true,
			"includeDate": false
		}
	},
	"docdash": {
		"sort": true
	}
}
